   ],
   "source": [
    "def second_largest(ls):\t\t\n",
    "\tlargest = second = float('-inf')\t\t\n",
    "\tfor i in ls:\t\t\n",
    "\t\tif i > largest:\t\t\n",
    "\t\t\tsecond = largest\t\t\n",
    "\t\t\tlargest = i\t\t\n",
    "\t\telif largest > i > second:\t\t\n",
    "\t\t\tsecond = i\t\t\n",
    "\treturn second\n",
    "print(second_largest([1,2,3,4,5]))"
   ]
  }
