   "source": [
    "def rem_dups(ls):\n",
    "\tnew = []\n",
    "\tfor i in ls:\n",
    "\t\tif i not in new:\n",
    "\t\t\tnew.append(i)\n",
    "\treturn new\n",
    "print(rem_dups([1,2,2,3,4,4,5]))"
   ]
