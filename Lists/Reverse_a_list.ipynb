   ],
   "source": [
    "def reverse_list(ls):\n",
    "\trev_ls=[]\n",
    "\tfor i in range(len(ls)-1,-1,-1):\n",
    "\t\trev_ls.append(ls[i])\n",
    "\treturn rev_ls\n",
    "print(reverse_list([1,2,3,4,5]))"
   ]
  }
