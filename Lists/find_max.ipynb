   "source": [
    "def find_max(ls):\n",
    "\tmax=ls[0]\n",
    "\tfor i in ls:\n",
    "\t\tif i>max:\n",
    "\t\t\tmax=i\n",
    "\treturn max\n",
    "print(find_max([1,2,3,4,5]))"
   ]
  }
